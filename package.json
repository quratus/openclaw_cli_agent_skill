{
  "name": "@sqncr/openclaw-cli-agent-skill",
  "version": "0.2.3",
  "description": "OpenClaw skill that delegates coding tasks to Kimi CLI agents in isolated git worktrees",
  "type": "module",
  "main": "bin/cli/main.js",
  "bin": {
    "cli-worker": "bin/cli/main.js"
  },
  "files": ["bin", "skills", "scripts", "SECURITY.md"],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "install-skill": "node scripts/install-skill.js",
    "test": "npm run build && node --test 'tests/unit/**/*.test.js'",
    "test:unit": "npm run build && node --test 'tests/unit/**/*.test.js'",
    "test:integration": "npm run build && node --test 'tests/integration/**/*.test.js'",
    "lint": "eslint src --ext .ts && prettier -c src"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": ["openclaw", "kimi", "cli", "skill"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quratus/openclaw_cli_agent_skill.git"
  },
  "homepage": "https://github.com/quratus/openclaw_cli_agent_skill#readme",
  "bugs": { "url": "https://github.com/quratus/openclaw_cli_agent_skill/issues" },
  "publishConfig": { "access": "public" },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  }
}
